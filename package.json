{
  "name": "snyk-api-import",
  "description": "Snyk import via API",
  "main": "dist/index.js",
  "bin": {
    "snyk-api-import": "dist/index.js"
  },
  "scripts": {
    "format:check": "prettier --check \"{package.json,tsconfig.json,src/**/*}.{js,ts,json,yml}\"",
    "format": "prettier --write \"{package.json,tsconfig.json,src/**/*}.{js,ts,json,yml}\"",
    "lint": "npm run format:check && npm run lint:eslint",
    "lint:eslint": "eslint --cache 'src/**/*.ts'",
    "test": "npm run lint && npm run build && npm run test:unit",
    "test:unit": "node --max-old-space-size=4096 ./node_modules/.bin/jest --maxWorkers=2 --silent --ci --detectOpenHandles --verbose --testLocationInResults",
    "test:unit:debug": "npm run build && DEBUG=* node --max-old-space-size=4096 ./node_modules/.bin/jest",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:watch": "tsc-watch --onSuccess 'npm run test:unit'",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "build:types": "tsc --emitDeclarationOnly",
    "bundle": "node scripts/build.js",
    "build": "npm run clean && tsc",
    "build-watch": "tsc -w",
    "prepare": "npm run build",
    "snyk-test": "snyk test",
    "pkg-binaries:macos": "npm run bundle && npx @yao-pkg/pkg dist/index.js -t node20-macos-x64 -o snyk-api-import-macos",
    "pkg-binaries:macos-arm": "npm run bundle && npx @yao-pkg/pkg dist/index.js -t node20-macos-arm64 -o snyk-api-import-macos-arm",
    "pkg-binaries:linux": "npm run bundle && npx @yao-pkg/pkg dist/index.js -t node20-linux-x64 -o snyk-api-import-linux",
    "pkg-binaries:alpine": "npm run bundle && npx @yao-pkg/pkg dist/index.js -t node20-alpine-x64 -o snyk-api-import-alpine",
    "pkg-binaries:windows": "npm run bundle && npx @yao-pkg/pkg dist/index.js -t node20-win-x64 -o snyk-api-import-win.exe",
    "pkg:all": "npm run bundle && npm run pkg-binaries:macos && npm run pkg-binaries:macos-arm && npm run pkg-binaries:linux && npm run pkg-binaries:alpine && npm run pkg-binaries:windows"
  },
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/snyk-api-import"
  },
  "author": "Snyk Tech Services",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=20"
  },
  "files": [
    "bin",
    "dist"
  ],
  "homepage": "https://github.com/snyk/snyk-api-import#readme",
  "dependencies": {
    "@gitbeaker/core": "35.8.1",
    "@gitbeaker/node": "35.8.1",
    "@octokit/auth-app": "^8.1.1",
    "@octokit/plugin-retry": "4.0.3",
    "@octokit/rest": "^22.0.1",
    "base-64": "^1.0.0",
    "bottleneck": "2.19.5",
    "bunyan": "1.8.15",
    "debug": "4.3.4",
    "lodash": "4.17.23",
    "micromatch": "4.0.8",
    "needle": "2.9.1",
    "p-map": "4.0.0",
    "parse-link-header": "2.0.0",
    "rimraf": "^5.0.7",
    "simple-git": "3.16.0",
    "sleep-promise": "8.0.1",
    "snyk-request-manager": "^1.9.0",
    "source-map-support": "^0.5.16",
    "split": "1.0.1",
    "tslib": "^2.6.0",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@octokit/types": "6.14.2",
    "@semantic-release/exec": "^6.0.3",
    "@types/base-64": "^1.0.0",
    "@types/bunyan": "1.8.6",
    "@types/debug": "4.1.5",
    "@types/jest": "^29.5.12",
    "@types/lodash": "4.17.5",
    "@types/micromatch": "4.0.6",
    "@types/needle": "2.0.4",
    "@types/node": "20.11.1",
    "@types/parse-link-header": "1.0.0",
    "@types/split": "1.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@yao-pkg/pkg": "^6.9.0",
    "babel-jest": "^30.2.0",
    "esbuild": "^0.25.11",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-check-file": "^3.3.0",
    "jest": "^29.7.0",
    "memfs": "^4.49.0",
    "nock": "^13.2.1",
    "prettier": "2.7.1",
    "semantic-release": "^24.2.9",
    "ts-jest": "^29.1.5",
    "tsc-watch": "^4.1.0",
    "typescript": "^5.9.3",
    "uuid": "9.0.0"
  },
  "overrides": {
    "snyk-request-manager": {
      "axios": "^1.11.0"
    },
    "glob": "^10.5.0",
    "qs": "^6.14.1"
  },
  "pkg": {
    "scripts": [
      "dist/**/*.js",
      "dist/**/lib/**/*.js",
      "node_modules/@octokit/**",
      "node_modules/@gitbeaker/**",
      "node_modules/snyk-request-manager/**",
      "node_modules/**/*"
    ],
    "assets": [
      "./node_modules/axios/dist/node/axios.cjs",
      "./node_modules/snyk-request-manager/**",
      "./node_modules/@octokit/**",
      "./node_modules/@gitbeaker/**",
      "./node_modules/dtrace-provider/**",
      "./node_modules/nan/**",
      "config/**/*",
      "node_modules/config/**/*"
    ],
    "targets": [
      "node20-macos-arm64",
      "node20-macos-x64",
      "node20-linux-x64",
      "node20-alpine-x64",
      "node20-win-x64"
    ]
  }
}
